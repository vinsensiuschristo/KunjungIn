generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id            Int       @id @default(autoincrement())
  email         String    @unique
  name          String
  password      String
  date_of_birth DateTime?
  address       String
  rating        Float
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}
